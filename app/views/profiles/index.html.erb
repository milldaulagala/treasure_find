<p id="notice"><%= notice %></p>

<h1>Profiles</h1>



  <images>
    <% @profiles.each do |profile| %>
      <div>
        <%= profile.first_name %><br/>
        <%= profile.last_name %><br/>
        <%= profile.address %><br/>
        <%= image_tag profile.image[:medium].url%><br/>
        
        <%= profile.introduction %><br/>
        <%= profile.mobile %><br/>
        <%= profile.category.name %><br/>
        <%= profile.user.email %><br/>
        <%= link_to 'Show', profile %><br/>
        <%= link_to 'Edit', edit_profile_path(profile) %><br/>
        <%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %><br/>
      </div>
    <% end %>
  </images>


<br>

<%= link_to 'New Profile', new_profile_path %>
