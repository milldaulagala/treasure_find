<p id="notice"><%= notice %></p>

<h1>Antiques</h1>




  <images>
    <% @items.each do |item| %>

      <div>
         <%= item.name %><br/>
         <%= item.description %><br/>
         <%= item.condition %><br/>
         <%= item.manufacture %><br/>
         <%= item.manufacture_date %><br/>
         <%= link_to (image_tag item.image[:medium].url), item %><br/>
         <%= item.dimension %><br/>
         <%= item.weight %><br/>
         <%= item.user.email %><br/>
         
         
        
      </div>
    <% end %>
  </images>


<br>

<%= link_to 'List New Treasure ', new_item_path, class: 'btn btn-primary' %>
